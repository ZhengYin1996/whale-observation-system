# Routes

GET     /                   controllers.ObservationController.index(request: Request)

GET     /createobservation  controllers.ObservationController.createObservation(request: Request)
GET     /deleteobservations  controllers.ObservationController.deleteAllObservations(request: Request)
GET     /observation        controllers.ObservationController.addObservation(request: Request)
GET     /whale              controllers.ObservationController.addWhale(request: Request)

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file   controllers.Assets.versioned(path="/public", file: Asset)

